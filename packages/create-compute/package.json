{
  "name": "create-compute",
  "version": "0.0.1",
  "description": "Create ComputeSDK projects with a single command",
  "author": "Garrison",
  "license": "MIT",
  "type": "module",
  "bin": {
    "create-compute": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ."
  },
  "keywords": [
    "create",
    "init",
    "scaffold",
    "computesdk",
    "sandbox",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/computesdk/computesdk.git"
  },
  "homepage": "https://github.com/computesdk/computesdk",
  "bugs": {
    "url": "https://github.com/computesdk/computesdk/issues"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.37.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}